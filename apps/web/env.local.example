## Required (Supabase)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

## Backend API base (optional)
NEXT_PUBLIC_BACKEND_URL=http://localhost:8000

## Server-side (optional)
SUPABASE_SERVICE_ROLE_KEY=

## Stripe (optional)
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
STRIPE_PRICE_ID_PRO=

## Observability (optional - Sentry)
# Public DSN for client-side reporting. If set, Sentry will initialize in `sentry.*.config.ts`.
NEXT_PUBLIC_SENTRY_DSN=
# Server-side DSN (can be the same as NEXT_PUBLIC_SENTRY_DSN).
SENTRY_DSN=
# e.g. production, preview, development
SENTRY_ENVIRONMENT=development
# Perf sampling (0.0 - 1.0)
SENTRY_TRACES_SAMPLE_RATE=0.05


